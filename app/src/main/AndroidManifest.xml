<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.persandaapps">

    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".AddPayment" />
        <activity android:name=".SearchPayment" />
        <activity android:name=".ViewPayment" />
        <activity android:name=".QueryPayment" />
        <activity android:name=".AddUtility" />
        <activity android:name=".AddRent" />
        <activity android:name=".AddDeposit" />
        <activity android:name=".ViewDeposit" />
        <activity android:name=".ViewRent" />
        <activity android:name=".ViewRentPopulate" />
        <activity android:name=".ViewUtility" />
        <activity android:name=".ViewUtilityPopulate" />
        <activity android:name=".ViewDue" />
    </application>

</manifest>